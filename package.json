{
	"name": "devclock",
	"type": "commonjs",
	"displayName": "devclock",
	"description": "",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.95.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"panel": [
				{
					"id": "devclock-details",
					"title": "devclock",
					"icon": "$(clock)"
				}
			]
		},
		"views": {
			"devclock-details": [
				{
					"type": "webview",
					"id": "devclock.sessionDetails",
					"name": "Session Details"
				}
			]
		},
		"commands": [
			{
				"command": "devclock.execDebug",
				"title": "devclock execDebug"
			},
			{
				"command": "devclock.showDashboard",
				"title": "Show Devclock Dashboard"
			},
			{
				"command": "devclock.showSessionDetails",
				"title": "Show Session Details",
				"category": "Devclock"
			},
			{
				"command": "devclock.deactivate",
				"title": "Deactivate Devclock"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "node esbuild.js --production",
		"dev": "node esbuild.js",
		"watch": "node esbuild.js --watch",
		"package": "npm run check-types && node esbuild.js --production",
		"check-types": "tsc --noEmit"
	},
	"devDependencies": {
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.9",
		"@types/node": "20.x",
		"@types/vscode": "^1.95.0",
		"@types/vscode-webview": "^1.57.5",
		"@typescript-eslint/eslint-plugin": "^8.10.0",
		"@typescript-eslint/parser": "^8.7.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"esbuild": "^0.24.0",
		"eslint": "^9.13.0",
		"glob": "^8.1.0",
		"npm-run-all": "^4.1.5",
		"typescript": "^5.6.3"
	},
	"dependencies": {}
}
